debug.svi.logs=0
log.tag.stats_log=0
persist.debug.trace=0
persist.sys.loglevel=0
ro.kernel.android.checkjni=0
persist.vendor.sys.modem.logging.enable=false
persist.anr.dumpthr=0
libc.debug.malloc=0
debug.egl.profiler=0
debug.enable.wl_log=0
debug.hwc.otf=0
debug.hwc_dump_en=0
